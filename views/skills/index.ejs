<%- include('../partials/header.ejs') %>

    <h1>Dev Skills</h1>
    <ul>
        <% skills.forEach(function (skill) { %>
            <li>
                <a href="/skills/<%= skill.id %>"><%= skill.name %></a>
                :
                <%= skill.level %>
            </li>
        <% }) %>
    </ul>
    <a href="/skills/new">Add Skill</a>

